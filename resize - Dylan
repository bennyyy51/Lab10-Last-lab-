import java.util.Arrays;

/*
 * A class for HashMaps with methods LF and resize.
 * @author Dylan Nguyen
 */
public class HashMap {
	
	private int size = 0;
	private int capacity = 0;
	private Object[] HashMap;
	
	public HashMap() {
		
	}
	
	/*
	 * Method to calculate the Loading Factor
	 * @author Dylan Nguyen
	 */
	private int LF() {		
		return size/capacity;
	}
	
	/*
	 * Method that resizes the Array.
	 * @author Dylan Nguyen
	 */
	public Object[] resize() {
		int newCapacity = 2 * HashMap.length;
		Object[] oldArray = HashMap;
		HashMap = new Object[newCapacity];
		
		for(int i = 0; i < oldArray.length; i++) {
			if(oldArray[i] != null) {
				HashMap.add(oldArray[i]);
			}
		}		
		return HashMap;
	}
}
